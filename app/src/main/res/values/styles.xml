<resources>

    <!-- Base application theme. -->
    <style name="AppTheme.Blue" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/blueThemeTitle</item>
        <item name="colorPrimaryDark">@color/blueThemeTitleDark</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <!-- Base application theme. -->
    <style name="AppTheme.nPink" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/noirPinkThemeTitle</item>
        <item name="colorPrimaryDark">@color/noirPinkThemeBackground</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <!-- Base application theme. -->
    <style name="AppTheme.Mint" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/mintThemeTitle</item>
        <item name="colorPrimaryDark">@color/mintThemeTitleDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <!-- Base application theme. -->
    <style name="AppTheme.sPink" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/softPinkThemeTitle</item>
        <item name="colorPrimaryDark">@color/softPinkThemeTitleDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

</resources>
